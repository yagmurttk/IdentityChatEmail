@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">
@await Component.InvokeAsync("_MessageHeadComponentPartial")
<body>
    @await Component.InvokeAsync("_MessagePreloaderComponentPartial")
    <div id="main-wrapper">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="card">
                        <div class="card-body">
                            @await Component.InvokeAsync("_MessageSidebarComponentPartial")
                            @RenderBody()
                        </div>
                    </div>
                </div>
            </div>
        </div>
        @await Component.InvokeAsync("_MessageFooterComponentPartial")
    </div>
    @await Component.InvokeAsync("_MessageScriptComponentPartial")
</body>
</html>